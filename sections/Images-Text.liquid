          >
            {%- for block in section.blocks -%}
              {% case block.type %}
               
                {%- when 'text' -%}
                   <a href="{{ section.settings.link }}">
                 {%- if block.settings.small_image_second != blank -%}
                            
                              <img
                                src="{{ block.settings.small_image_second | img_url: 'medium' }}"
                                alt="{{ block.settings.small_image_second.alt | escape }}"
                                loading="lazy" class="img__1"
                              >
                            
                          {%- endif -%}   
                   </a>
              {%- endcase -%}
            {%- endfor -%}
          </div>
        </div>
        <div class="image-with-text__media-item1 image-with-text__media-item--{{ section.settings.desktop_image_width }} image-with-text__media-item--{{ section.settings.desktop_content_position }}">
          <a href="{{ section.settings.link }}">
